<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://xamarin.com/schemas/2014/forms"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:views="clr-namespace:PaulSpotifyApp.Views;assembly=PaulSpotifyApp"
       x:Class="PaulSpotifyApp.MainPage">
    <TabBar>
        <Tab Title="1"
             Icon="un.png">
            <ShellContent ContentTemplate="{DataTemplate views:PageUne}" />
        </Tab>
        <Tab Title="2"
             Icon="deux.png">
            <ShellContent ContentTemplate="{DataTemplate views:PageDeux}" />
        </Tab>
        <Tab Title="3"
             Icon="trois.png">
            <ShellContent ContentTemplate="{DataTemplate views:PageTrois}" />
        </Tab>
        <Tab Title="4"
             Icon="quatre.png">
            <ShellContent ContentTemplate="{DataTemplate views:PageQuatre}" />
        </Tab>
    </TabBar>
</Shell>
